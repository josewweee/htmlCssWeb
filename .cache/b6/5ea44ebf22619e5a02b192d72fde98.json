team\n6. portfolio\n7. blog\n8. questions\n9. testimonials\n9. contact\n10. footer\n*/\n\n/* MAIN BANNER */\n#main-banner {\n  background-image: url(../resources/home-bg-2.jpg);\n  background-position: left 50% bottom 20%;\n  background-size: cover;\n  background-repeat: no-repeat;\n  background-origin: border-box;\n  background-clip: border-box;\n  width: 100vw;\n  height: 46vh;\n\n  .banner-content {\n    text-align: center;\n    padding-top: 10vh;\n    margin-left: 2%;\n  }\n\n  h2 {\n    color: white;\n  }\n\n  p {\n    color: white;\n    margin-top: 1rem;\n  }\n\n  .bg-buttons {\n    button {\n      background: none;\n      color: white;\n      font-weight: bold;\n      border: 3px solid white;\n      padding: 0.5rem;\n      width: 10rem;\n      margin-top: 4rem;\n    }\n    button:hover {\n      background: #ffffff;\n      color: green;\n      border: 3px solid #ffffff;\n      outline: none;\n    }\n    button:focus {\n      outline: none;\n    }\n    button:active {\n      background-color: orange;\n      border: none;\n      outline: none;\n      color: white;\n    }\n  }\n\n  .bg-button-2 {\n    display: none;\n  }\n  @media (min-width: 768px) {\n    .bg-button-2 {\n      display: inline-block;\n      margin-left: 1rem;\n      background: #17b87e;\n      border: 3px solid #17b87e;\n    }\n\n    .banner-content {\n      padding-top: 10%;\n    }\n  }\n}\n/* END MAIN BANNER */\n\n/* NAV BAR */\n.main-header {\n  display: block;\n  position: relative;\n\n  .header-list--desktop {\n    display: none !important;\n  }\n\n  @media (min-width: 768px) {\n    .main-header-inital {\n      padding: 0.2rem;\n      align-items: center;\n    }\n    .toggle-button {\n      display: none;\n    }\n\n    .header-list--mobile {\n      display: none;\n    }\n\n    .header-list--desktop {\n      display: flex !important;\n      right: 0;\n      margin-top: 1.3rem;\n      margin-bottom: 0;\n      width: auto;\n      li {\n        margin-right: 3rem;\n      }\n    }\n  }\n}\n\n.main-header-inital {\n  display: flex;\n  box-shadow: 0px 3px 2px 0px rgb(132 123 123 / 10%);\n  position: relative;\n  justify-content: space-between;\n  width: 100%;\n}\n\n.main-header--title {\n  margin: 0.4rem;\n  margin-left: 4vw;\n  font-weight: bold;\n}\n\n//menu hamburguesa\n.toggle-button {\n  width: 3rem;\n  background: transparent;\n  border: none;\n  cursor: pointer;\n  padding-top: 0;\n  padding-bottom: 0;\n  height: 2vh;\n  margin: 0.4rem;\n  display: block;\n}\n\n.toggle-button:focus {\n  outline: none;\n}\n\n.toggle-button__bar {\n  width: 1.5rem;\n  height: 2px;\n  background: green;\n  display: block;\n  margin: 0.3rem 0;\n}\n//fin menu hamburguesa\n\n.header-list {\n  list-style: none;\n  margin-top: 4rem;\n  position: relative;\n  width: 100%;\n\n  li {\n    margin-bottom: 1.5rem;\n    text-align: center;\n\n    a {\n      color: gray;\n    }\n\n    a:focus,\n    a:hover {\n      color: green;\n      outline: none;\n      text-decoration: none;\n    }\n  }\n}\n/* END NAV BAR */\n\n/* SERVICES */\n\n#services {\n  margin-top: 20vh;\n}\n\n.services-introduction {\n  margin-bottom: 3rem;\n  h1 {\n    font-weight: bold;\n  }\n}\n.services-description--tab {\n  margin-bottom: 1.5rem;\n}\n\n.services-description--titles {\n  display: flex;\n  svg {\n    font-size: 2.5rem;\n  }\n\n  .col-1 {\n    margin-right: 10%;\n  }\n}\n\n.services-description--paragraph {\n  color: gray;\n  margin-left: 18.9%;\n  padding-left: 15px;\n}\n/* END SERVICES */\n\n/* ABOUT */\n\n#about {\n  background-color: #f9f9f9;\n  margin-top: 5rem;\n}\n\n.about-content {\n  display: flex;\n  flex-wrap: wrap;\n  padding-top: 6rem;\n\n  h1 {\n    font-weight: bold;\n  }\n\n  @media (min-width: 768px) {\n    flex-wrap: nowrap;\n  }\n\n  .about-description {\n    width: 100%;\n  }\n\n  .about-video {\n    width: 100%;\n  }\n}\n/* END ABOUT */\n"},"lineCount":null}},"error":null,"hash":"28cda9efd7232ab4f4d89d02beb6e10b","cacheData":{"env":{}}}